package java_study.chapter05;

public class CallByReferenceEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] data = { 100, 200 };

		System.out.printf("%d %d\n", data[0], data[1]);
	}

	static void swap(int[0] data) {
		int temp;
		temp = data[0];
		data[0] =data[1];
		data[1] =temp;
		System.out.printf("%d %d\n", data[0], data[1]);
	}

}
