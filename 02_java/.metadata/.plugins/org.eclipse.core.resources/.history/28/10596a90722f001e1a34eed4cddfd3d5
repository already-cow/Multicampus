package java_study.chapter03;

public class StringCompare {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str1 = "안녕하세요";
		String str2 = "안녕하세요";
		
		if(str1 == str2) {
			System.out.println("두 문자열 동등");}
		} else {
			System.out.println("두 문자열 다름");
		}

}
