package java_study.chapter2;

public class MyHome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 변수명은 무조건 소문자로 시작
		// 연결되는 단어가 있으면 낙타표기법
		int myAge = 100;
		String myTel = "011"; // java는 큰따옴표
		String ssn = "880101";
		final String COMPANY = "메가더조은"; // const 상수는 대문자
		COMPANY = "mega";
		//final => 수정불가 (const)
	}

}
