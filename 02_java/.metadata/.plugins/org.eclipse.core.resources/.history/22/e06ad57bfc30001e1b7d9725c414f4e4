package java_study.chapter05;

public class CallByReferenceEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] data = { 100, 200 };

		System.out.println("%d %d\n", data[0], data[1]);
	}

	static void swap(int[0] data) {
		int temp;
		temp = x;
		x =y;
		temp=y;
		System.out.println("%d %d\n", x, y);
	}

}
