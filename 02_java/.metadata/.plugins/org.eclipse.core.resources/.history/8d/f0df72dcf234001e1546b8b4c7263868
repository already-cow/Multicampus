package chapter06.sec5;

public class AccountUse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Account dad = new Account();
		dad.name = "홍길동";
		dad.accountName = "튼튼적금";
		dad.balance = 5000;

		dad.deposit(3000);
		dad.widthdraw(2000);

		System.out.println(dad);
	}

}
