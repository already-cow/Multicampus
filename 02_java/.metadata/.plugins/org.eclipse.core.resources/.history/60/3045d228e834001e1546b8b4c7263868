package chapter06.sec5;

public class Account {
	String name;
	String accountName;
	int balance;

	public void deposit(int amount) {
		balance += amount;
		System.out.printf("%s이 %d원을 입금하다.\n", name, balance);
	}

	public void widthdraw(int amount) {
		balance -= amount;
		System.out.printf("%s이 %d원을 출금하다.\n", name, amount);
	}

}
